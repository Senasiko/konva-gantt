{
  "name": "@konva-gantt/core",
  "version": "0.0.1",
  "description": "",
  "private": false,
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./shapes/*": {
      "default": "./dist/shapes/*.js",
      "types": "./dist/shapes/*.d.ts"
    },
