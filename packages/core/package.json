  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "konva": "^9.2.3",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.3",
    "pinia": "^2.1.7",
    "vue": "^3.3.9"
  },
  "devDependencies": {
    "@konva-gantt/config-typescript": "workspace:*",
    "@types/lodash": "^4.14.202",
    "typescript": "^5.3.2"
  }
